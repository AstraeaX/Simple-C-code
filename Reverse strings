#include <stdio.h>
#include <string.h>
#include <ctype.h>

int main(void) {

   char input[10][50];
   int x;
   
   for (int i = 0; i < 10; i++) {
      scanf(" %[^\n]s", input[i]); \\ Scan a string with spaces until newline
      if (strcmp(input[i], "quit") == 0 || strcmp(input[i], "q") == 0 || strcmp(input[i], "Quit") == 0 ) {
         x = i;
        break;
      }
   }
   
   for (int i = 0; i < x; i++) {
      for (int j = strlen(input[i]) - 1; j >= 0; j--) {
         
         printf("%c", input[i][j]);
      }
      printf("\n");
   }
   
   return 0;
}
